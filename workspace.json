{
  "version": 1,
  "projects": {
    "portfolio-site": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/portfolio-site",
      "sourceRoot": "apps/portfolio-site/src",
      "prefix": "portfolio",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/portfolio-site",
            "index": "apps/portfolio-site/src/index.html",
            "main": "apps/portfolio-site/src/main.ts",
            "polyfills": "apps/portfolio-site/src/polyfills.ts",
            "tsConfig": "apps/portfolio-site/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/portfolio-site/src/favicon.ico",
              "apps/portfolio-site/src/robots.txt",
              "apps/portfolio-site/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/portfolio-site/src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": ["hammerjs"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/portfolio-site/src/environments/environment.ts",
                  "with": "apps/portfolio-site/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "portfolio-site:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "portfolio-site:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "portfolio-site:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/portfolio-site/tsconfig.app.json",
              "apps/portfolio-site/tsconfig.spec.json",
              "apps/portfolio-site/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/portfolio-site/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/portfolio-site/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "portfolio-site-e2e": {
      "root": "apps/portfolio-site-e2e",
      "sourceRoot": "apps/portfolio-site-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/portfolio-site-e2e/cypress.json",
            "tsConfig": "apps/portfolio-site-e2e/tsconfig.e2e.json",
            "devServerTarget": "portfolio-site:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "portfolio-site:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/portfolio-site-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/portfolio-site-e2e/**/*"]
          }
        }
      }
    },
    "home": {
      "projectType": "library",
      "root": "libs/home",
      "sourceRoot": "libs/home/src",
      "prefix": "portfolio",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/home/tsconfig.lib.json", "libs/home/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/home/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/home/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "layout": {
      "projectType": "library",
      "root": "libs/layout",
      "sourceRoot": "libs/layout/src",
      "prefix": "portfolio",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/layout/tsconfig.lib.json", "libs/layout/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/layout/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/layout/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "shared": {
      "projectType": "library",
      "root": "libs/shared",
      "sourceRoot": "libs/shared/src",
      "prefix": "portfolio",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/shared/tsconfig.lib.json", "libs/shared/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/shared/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "material": {
      "projectType": "library",
      "root": "libs/material",
      "sourceRoot": "libs/material/src",
      "prefix": "portfolio",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/material/tsconfig.lib.json", "libs/material/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/material/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/material/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "strict": true
    }
  },
  "defaultProject": "portfolio-site"
}
