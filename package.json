{
  "name": "portfolio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "build:prod": "nx build --prod",
    "build:prod:stats": "nx build --stats-json --prod",
    "analyze:portfolio": "webpack-bundle-analyzer dist/apps/portfolio-site/stats.json",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write && npm run stylelint:fix",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "prettier": "npm run prettier:ts && npm run prettier:html",
    "prettier:ts": "prettier --write --parser 'typescript' '{apps,libs}/*/src/**/*.ts'",
    "prettier:html": "prettier --write --parser 'angular' '{apps,libs}/*/src/**/*.html'",
    "stylelint": "stylelint '{apps,libs}/*/src/**/*.{css,scss}'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "textlint": "textlint '{*.md,{apps,libs}/*/src/**/*.{html,md,txt}'}",
    "textlint:fix": "npm run textlint -- --fix",
    "gzipper": "gzipper compress ./dist/apps ./dist/gzipped",
    "gzipper:s3": "gzipper compress --output-file-format [filename].[ext] ./dist/apps ./dist/gzipped-s3",
    "serve:portfolio": "http-server dist/apps/portfolio-site/",
    "serve:portfolio:gzipped": "http-server dist/gzipped/apps/portfolio-site/ -g"
  },
  "lint-staged": {
    "*.html": [
      "prettier --write --parser 'angular'",
      "git add"
    ],
    "*.ts": [
      "tslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss --fix",
      "git add"
    ],
    "*.{json,yaml,graphql}": [
      "prettier --write",
      "git add"
    ],
    "*.{html,md,txt}": [
      "textlint --fix",
      "textlint",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.1.0",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/forms": "^10.1.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/platform-server": "^10.1.0",
    "@angular/router": "^10.1.0",
    "@angular/service-worker": "^10.1.0",
    "@nrwl/angular": "10.3.3",
    "dayjs": "^1.9.6",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.5",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/compiler-cli": "^10.1.0",
    "@angular/language-service": "^10.1.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@nrwl/cli": "10.3.3",
    "@nrwl/cypress": "10.3.3",
    "@nrwl/jest": "10.3.3",
    "@nrwl/workspace": "10.3.3",
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": "^1.0.1",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "firebase-tools": "^8.16.2",
    "gzipper": "^4.3.0",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "hysky": "^1.0.1",
    "imagemin-lint-staged": "^0.4.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "^10.5.1",
    "prettier": "2.0.4",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "textlint": "^11.7.6",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-plugin-html": "^0.2.0",
    "textlint-rule-date-weekday-mismatch": "^1.0.5",
    "textlint-rule-first-sentence-length": "^1.0.1",
    "textlint-rule-incremental-headers": "^0.2.0",
    "textlint-rule-ja-hiragana-fukushi": "^1.2.0",
    "textlint-rule-ja-hiragana-hojodoushi": "^1.0.4",
    "textlint-rule-ja-hiragana-keishikimeishi": "^1.0.2",
    "textlint-rule-ja-no-abusage": "^2.0.1",
    "textlint-rule-ja-no-mixed-period": "^2.1.1",
    "textlint-rule-ja-no-redundant-expression": "^3.0.2",
    "textlint-rule-ja-unnatural-alphabet": "^2.0.1",
    "textlint-rule-max-appearence-count-of-words": "^1.0.1",
    "textlint-rule-max-comma": "^1.0.4",
    "textlint-rule-max-kanji-continuous-len": "^1.1.1",
    "textlint-rule-max-length-of-title": "^1.0.1",
    "textlint-rule-max-number-of-lines": "^1.0.3",
    "textlint-rule-max-ten": "^2.0.4",
    "textlint-rule-ng-word": "^1.0.0",
    "textlint-rule-no-dead-link": "^4.7.0",
    "textlint-rule-no-double-negative-ja": "^1.0.6",
    "textlint-rule-no-doubled-conjunction": "^1.0.3",
    "textlint-rule-no-doubled-conjunctive-particle-ga": "^1.1.1",
    "textlint-rule-no-doubled-joshi": "^3.8.0",
    "textlint-rule-no-dropping-the-ra": "^2.0.0",
    "textlint-rule-no-empty-section": "^1.1.0",
    "textlint-rule-no-exclamation-question-mark": "^1.1.0",
    "textlint-rule-no-hankaku-kana": "^1.0.2",
    "textlint-rule-no-mixed-zenkaku-and-hankaku-alphabet": "^1.0.1",
    "textlint-rule-no-nfd": "^1.0.2",
    "textlint-rule-no-start-duplicated-conjunction": "^2.0.2",
    "textlint-rule-no-surrogate-pair": "^1.0.1",
    "textlint-rule-no-todo": "^2.0.1",
    "textlint-rule-prefer-tari-tari": "^1.0.3",
    "textlint-rule-sentence-length": "^2.2.0",
    "textlint-rule-terminology": "^2.1.4",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.0.3",
    "webpack-bundle-analyzer": "^4.1.0"
  }
}
